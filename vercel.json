{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/client/index.html"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 10
        }
    }
}